<PROPERTY KEY="FNT_SQLSELECT">Select
  cad_pessoa.id_pessoa,
  cad_pessoa.cpf_pessoa,
  cad_pessoa.nome_social,
  cad_pessoa.telefone,
  cad_pessoa.status_cadastro,
  fr_usuario.usr_nome,
  fr_usuario.usr_email,
  Regexp_Replace(cad_pessoa.cpf_pessoa, '(\d{3})(\d{3})(\d{3})', '\1.\2.\3-') As cpfformatado,
  cad_estadocivil.estado_civil,
  cad_genero.nome_genero,
  To_Char(cad_pessoa.rendamensal_declarada, 'L9G999G990D99') As rendaformatada,
  cad_pessoa.status_cadastro As status,
  cad_pessoa.rating
From
  cad_pessoa
  Inner Join
  fr_usuario On cad_pessoa.id_pessoa = fr_usuario.id_pessoa
  Left Join
  cad_estadocivil On cad_pessoa.id_estadocivil = cad_estadocivil.id_estadocivil
  Left Join
  cad_genero On cad_pessoa.id_genero = cad_genero.id_genero
Where
  cad_pessoa.status_cadastro = 1

/*WHERE_ADD*/
Order By
  cad_pessoa.id_pessoa

/*ORDER_ADD*/
</PROPERTY>
