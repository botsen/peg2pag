<PROPERTY KEY="FNT_SQLSELECT">Select
  l.idlancamento,
  l.idtransacao,
  l.nuparcela,
  Trim(To_Char(l.vljuros, '999G999G999d99')) || '%' As juros,
  'R$ ' || Trim(To_Char(l.vlparcelaspread, '999G999G999d99')) As valor,
  l.vlmultaprcant,
  Case When ft.sttransacao = 1 Then cst.sttransac Else csl.stlancamento End As status,
  l.stlancamento,
  l.dtultatu,
  l.vlparcelaspread
From
  fin_lancamento l
  Left Join
  fin_transacao ft On ft.idtransacao = l.idtransacao
  Left Join
  cad_status_transacao cst On cst.idsttransac = ft.sttransacao
  Left Join
  cad_status_lancamento csl On csl.idstlancamento = l.stlancamento

/*WHERE_NEW*/
Order By
  l.nuparcela

/*ORDER_ADD*/
</PROPERTY>
