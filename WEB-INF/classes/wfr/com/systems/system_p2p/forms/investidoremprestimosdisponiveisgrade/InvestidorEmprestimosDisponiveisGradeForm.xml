<PROPERTY KEY="FNT_SQLSELECT">Select
  fin_transacao.idtransacao,
  fin_transacao.vltransacao,
  'R$ ' || Trim(To_Char(fin_transacao.vltransacao, '999G999G999D99')) As valorsolicitado,
  fin_transacao.dttransacao,
  fin_transacao.idinvestidor,
  fin_transacao.idtomador,
  fin_transacao.sttransacao,
  fin_transacao.qtparcelas,
  fin_transacao.dtultatu,
  fin_transacao.vlcotasrec,
  'R$ ' || Trim(To_Char(fin_transacao.vlcotasrec, '999G999G999D99')) As valorcotas,
  fin_transacao.dtprimpag,
  fin_transacao.guid,
  fin_transacao.stativo,
  fin_transacao.vljuros,
  Trim(To_Char(fin_transacao.vljuros, '999G999G999D99')) || '% a.m.' As valorjuros,
  fin_transacao.vlrating,
  fin_tomador.scoreatu,
  Coalesce(cad_pessoa.nome_social, fu.usr_nome) As nome_social,
  'R$ ' || Trim(To_Char(fin_transacao.vltotalinvestidor, '999G999G999D99')) As valorinvestidor,
  fin_transacao.qtcotas,
  'R$ ' || Trim(To_Char(fin_transacao.vlcotasimples, '999G999G999D99')) As vlcotasimples
From
  fin_transacao
  Inner Join
  fin_tomador On fin_transacao.idtomador = fin_tomador.idtomador
  Inner Join
  cad_pessoa On fin_tomador.idpessoa = cad_pessoa.id_pessoa
  Inner Join
  fr_usuario fu On fu.id_pessoa = cad_pessoa.id_pessoa
Where (
  (fin_transacao.idtransacao Not In (Select vie.idtransacao From vw_investidor_emprestimo vie Where vie.id_pessoa = :SESSAO_sessaoIdPessoa)) And
  (fin_transacao.sttransacao = 1) And
  (fin_transacao.stativo = 1))


/*WHERE_ADD*/
/*ORDER_NEW*/
